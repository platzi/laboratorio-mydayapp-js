		"raw-loader": "^4.0.2",
		"style-loader": "3.3.1",
		"stylelint": "^16.1.0",
		"stylelint-config-standard-scss": "^12.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.0",
		"stylelint-scss": "^6.0.0",
		"webpack": "5.73.0",
